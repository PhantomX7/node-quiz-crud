<% include ../partials/headerAdmin %>
  <div class="container">
    <h2 class=" ml-3 display-3">
        class
    </h2>
    <div class="col-md-12 container">
      <% classes.forEach(function(classs){ %>
      <div class="border border-secondary rounded p-3 m-2">
          <div class="row">
            <div class="caption col-12">
              <h3><a href="/admin/class/<%= classs._id %>" class="text-dark"><strong><%= classs.kelas %></strong></a></h3>
              <p>Date: <%= moment(classs.tanggal).format("MMM Do YY") %></p>
              <div>
                   status: <%= classs.disetujui?"Approved":"Not Approved" %>
              </div>
              <div>
                   Requested by: <%= classs.nama %>
              </div>

            </div>
          </div>
        </div>
      <% }) %>
    </div>
  </div>



<% include ../partials/footerAdmin %>
